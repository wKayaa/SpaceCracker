{
  "scanner": {
    "threads": 100,
    "rate_limit": 50,
    "timeout": 8,
    "max_retries": 2,
    "random_delay_min": 0.1,
    "random_delay_max": 0.5,
    "user_agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
  },
  "performance": {
    "auto_detect": false,
    "default_mode": "vps",
    "force_http": true,
    "enable_progress_updates": true,
    "telegram_update_interval": 30,
    "memory_cleanup_interval": 300
  },
  "modules": {
    "ggb_scanner": true,
    "js_scanner": true,
    "git_scanner": true,
    "cve_exploits": true,
    "path_scanner": true,
    "laravel_scanner": true,
    "smtp_scanner": true
  },
  "telegram": {
    "enabled": true,
    "bot_token": "YOUR_BOT_TOKEN_HERE",
    "chat_id": "YOUR_CHAT_ID_HERE",
    "send_only_valid": true,
    "progress_updates": true,
    "completion_notifications": true
  },
  "output": {
    "json_file": "results.json",
    "txt_file": "results.txt",
    "csv_file": "results.csv",
    "verbose": true
  },
  "validation": {
    "check_aws": true,
    "check_sendgrid": true,
    "check_smtp": true,
    "check_stripe": true,
    "check_github": true,
    "check_mailgun": true,
    "check_postmark": true,
    "check_sparkpost": true,
    "check_brevo": true
  }
}